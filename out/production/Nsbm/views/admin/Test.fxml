<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import java.lang.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.collections.FXCollections?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.admin.TestController">
    <children>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <Label fx:id="titleLabel" text="Tests" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="199.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
            <children>
                <TableView fx:id="testTable" onMouseClicked="#getSelectedRow" prefHeight="164.0" prefWidth="666.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                    <columns>
                        <TableColumn prefWidth="42.0" text="ID">
                            <cellValueFactory>
                                <PropertyValueFactory property="test_id" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="Date">
                            <cellValueFactory>
                                <PropertyValueFactory property="date" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="Start">
                            <cellValueFactory>
                                <PropertyValueFactory property="start_time" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="End">
                            <cellValueFactory>
                                <PropertyValueFactory property="end_time" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="Subject">
                            <cellValueFactory>
                                <PropertyValueFactory property="subject" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="75.0" text="Type">
                            <cellValueFactory>
                                <PropertyValueFactory property="type" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                </TableView>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="600.0">
            <children>
                <DatePicker fx:id="dateField" promptText="Date">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </DatePicker>
                <TextField fx:id="startField" promptText="Start time">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </TextField>
                <TextField fx:id="endField" promptText="End time">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </TextField>
                <ComboBox fx:id="subjectBox" prefWidth="150.0" promptText="Subject">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </ComboBox>
                <ComboBox fx:id="typeBox" prefWidth="150.0" promptText="Type">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="PA" />
                            <String fx:value="WA" />
                            <String fx:value="EX" />
                        </FXCollections>
                    </items>
                </ComboBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button fx:id="addButton" alignment="CENTER" mnemonicParsing="false" onAction="#addTest" text="Add">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="editButton" alignment="CENTER" mnemonicParsing="false" onAction="#editTest" text="Edit">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="removeButton" alignment="CENTER" mnemonicParsing="false" onAction="#removeTest" text="Remove">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <VBox.margin>
                <Insets left="10.0" />
            </VBox.margin>
           <children>
               <Button fx:id="mainMenuButton" mnemonicParsing="false" onAction="#toMainPanel" text="Main Menu">
                   <HBox.margin>
                       <Insets />
                   </HBox.margin>
               </Button>
           </children>
        </HBox>
    </children>
</VBox>
